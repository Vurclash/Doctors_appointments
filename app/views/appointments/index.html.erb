<h1><%= @doctor.full_name %> Appointments</h1>
<%= link_to "Add Appointment", new_doctor_appointment_path(@doctor) %>
<hr />

<h2>Doctor</h2>
<ul>
  <% @medics.each do |doc| %>
    <li>
      <%= doc.user.full_name %>
      <br />
      <%= link_to "Remove Appointment", doctor_appointment_path(@doctor, doc), method: :delete %>
    </li>
  <% end %>
</ul>
